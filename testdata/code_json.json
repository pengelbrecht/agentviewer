  "api": {
    "endpoints": [
      {
        "method": "GET",
        "path": "/api/v1/users",
        "description": "List all users",
        "authentication": "required",
        "rateLimit": {
          "requests": 100,
          "window": "1m"
        },
        "parameters": {
          "query": [
            {
              "name": "page",
              "type": "integer",
              "required": false,
              "default": 1
            },
            {
              "name": "limit",
              "type": "integer",
              "required": false,
              "default": 20,
              "min": 1,
              "max": 100
            },
            {
              "name": "sort",
              "type": "string",
              "required": false,
              "enum": ["createdAt", "updatedAt", "username"]
            }
          ]
        },
        "response": {
          "success": {
            "status": 200,
            "body": {
              "data": "array<User>",
              "pagination": {
                "page": "integer",
                "limit": "integer",
                "total": "integer",
                "hasMore": "boolean"
              }
            }
          },
          "errors": [
            { "status": 401, "message": "Unauthorized" },
            { "status": 429, "message": "Rate limit exceeded" },
            { "status": 500, "message": "Internal server error" }
          ]
        }
      },
      {
        "method": "POST",
        "path": "/api/v1/users",
        "description": "Create a new user",
        "authentication": "required",
        "permissions": ["users:create"],
        "body": {
          "contentType": "application/json",
          "schema": {
            "username": {
              "type": "string",
              "required": true,
              "minLength": 3,
              "maxLength": 50,
              "pattern": "^[a-z0-9_]+$"
            },
            "email": {
              "type": "string",
              "required": true,
              "format": "email"
            },
            "password": {
              "type": "string",
              "required": true,
              "minLength": 8
            }
          }
        }
      }
    ]
  },

  "config": {
    "server": {
      "host": "0.0.0.0",
      "port": 8080,
      "timeout": 30000,
      "keepAlive": true,
      "cors": {
        "enabled": true,
        "origins": ["http://localhost:3000", "https://app.example.com"],
        "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
        "headers": ["Content-Type", "Authorization", "X-Request-ID"]
      },
      "compression": {
        "enabled": true,
        "threshold": 1024,
        "level": 6
      }
    },
    "database": {
      "type": "postgresql",
      "host": "localhost",
      "port": 5432,
      "name": "myapp",
      "pool": {
        "min": 2,
        "max": 10,
        "idle": 10000
      },
      "ssl": {
        "enabled": true,
        "rejectUnauthorized": false
      }
    },
    "cache": {
      "type": "redis",
      "host": "localhost",
      "port": 6379,
      "ttl": 3600,
      "prefix": "myapp:"
    },
    "logging": {
      "level": "info",
      "format": "json",
      "outputs": ["stdout", "file"],
      "file": {
        "path": "/var/log/myapp/app.log",
        "maxSize": "100MB",
        "maxFiles": 10
      }
    },
    "features": {
      "darkMode": true,
      "betaFeatures": false,
      "experimentalApi": false,
      "maintenance": false
    }
  },

